{
	"name": "@mjs/token",
	"version": "0.0.2",
	"private": true,
	"author": {
		"name": "Lucas Verdiell",
		"email": "lucas@smat.io",
		"url": "https://lucasverdiell.xyz"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"clean": "rimraf .next node_modules",
		"build-deps": "turbo run build --filter @mjs/utils",
		"generate": "pnpm run build-deps && pnpm run db:generate",
		"db:better-auth:migrate": "dotenv -e .env.local  -- npx @better-auth/cli@latest generate",
		"db:generate": "dotenv -e .env.local -- npx prisma generate",
		"db:studio": "dotenv -e .env.local -- npx prisma studio",
		"db:migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev",
		"db:migrate:reset": "dotenv -e .env.local -- npx prisma migrate reset",
		"db:migrate:prod": "dotenv -e .env.production.local -- npx prisma migrate deploy",
		"db:push": "dotenv -e .env.local -- npx prisma db push",
		"db:push:stage": "dotenv -e .env.stage.local -- npx prisma db push",
		"build:bundle": "ANALYZE=true next build",
		"knip": "knip",
		"test": "vitest",
		"e2e": "playwright test",
		"e2e:debug": "PWDEBUG=1 playwright test",
		"e2e:console": "PWDEBUG='console' playwright test",
		"e2e:renew": " RENEW_STATE=true NODE_ENV=test playwright test --project=chromium",
		"e2e:codegen": "npx playwright codegen",
		"e2e:ci": "RENEW_STATE=true NODE_ENV=test playwright test --project=chromium",
		"e2e:extract-auth-state": "tsx ./__tests__/e2e/scripts/extract-auth-state.ts",
		"vercel-build": "npx prisma generate && turbo run build",
		"_vercel-build:prod": "npx prisma generate && npx prisma migrate deploy && next build",
		"db:seed": "dotenv -e .env.local -- npx prisma db seed -- --environment development",
		"db:seed:test": "dotenv -e .env.test -- npx prisma db seed -- --environment test",
		"db:seed:stage": "dotenv -e .env.stage.local -- npx prisma db seed -- --environment stage",
		"db:seed:prod": "dotenv -e .env.production.local -- npx prisma db seed -- --environment production",
		"db:reset": "pnpm run db:push --force-reset && pnpm run db:seed && pnpm run db:generate",
		"db:reset:stage": "dotenv -e .env.stage.local -- npx prisma migrate deploy && pnpm run db:seed:stage && pnpm run db:generate",
		"db:reset:test": "dotenv -e .env.test -- npx prisma db push && pnpm run db:generate",
		"db:deploy:stage": "dotenv -e .env.stage.local -- npx prisma migrate deploy",
		"db:deploy:prod": "dotenv -e .env.production.local -- npx prisma migrate deploy",
		"check-types": "tsc -p tsconfig.json --noEmit"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"dependencies": {
		"@documenso/sdk-typescript": "^0.3.1",
		"@epic-web/invariant": "^1.0.0",
		"@google-cloud/storage": "^7.16.0",
		"@heliofi/checkout-react": "^4.0.2",
		"@mjs/tailwind-config": "workspace:*",
		"@mjs/transactional": "workspace:*",
		"@mjs/typescript-config": "workspace:*",
		"@mjs/ui": "workspace:*",
		"@mjs/utils": "workspace:*",
		"@prisma/client": "6.8.2",
		"@prisma/extension-accelerate": "^2.0.0",
		"@react-email/components": "0.0.41",
		"@t3-oss/env-nextjs": "^0.13.4",
		"@tanstack/react-query": "^5.76.1",
		"@tanstack/react-store": "^0.7.3",
		"@vercel/analytics": "^1.5.0",
		"@vercel/functions": "^2.2.2",
		"@vercel/og": "^0.6.8",
		"cacheable": "^1.10.1",
		"canvas-confetti": "^1.9.3",
		"decimal.js": "^10.6.0",
		"deepmerge-ts": "^7.1.5",
		"ethers": "^6.15.0",
		"handlebars": "^4.7.8",
		"image-size": "^2.0.2",
		"lucide-react": "^0.511.0",
		"luxon": "^3.4.4",
		"mime-types": "^2.1.35",
		"next": "15.5.7",
		"next-intl": "^4.1.0",
		"next-safe-action": "^7.10.8",
		"next-themes": "^0.4.6",
		"nuqs": "^2.4.3",
		"pino": "^9.7.0",
		"react": "19.1.0",
		"react-camera-pro": "^1.4.0",
		"react-dom": "^19.1.0",
		"resend": "^4.5.1",
		"sanitize-html": "^2.17.0",
		"server-only": "^0.0.1",
		"sharp": "^0.34.3",
		"thirdweb": "^5.114.0",
		"usehooks-ts": "^3.1.1",
		"xlsx": "^0.18.5",
		"zod": "^3.25.76",
		"zod-prisma-types": "^3.2.4"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@faker-js/faker": "^9.9.0",
		"@next/bundle-analyzer": "^15.3.0",
		"@playwright/test": "latest",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/canvas-confetti": "^1.9.0",
		"@types/luxon": "^3.4.2",
		"@types/mime-types": "^2.1.4",
		"@types/node": "^22.15.19",
		"@types/react": "^19.1.5",
		"@types/react-dom": "^19.1.5",
		"@types/sanitize-html": "^2.16.0",
		"@types/sinon": "^17.0.3",
		"@vitejs/plugin-react": "^4.7.0",
		"dotenv": "^16.5.0",
		"dotenv-cli": "^8.0.0",
		"eslint-config-next": "^15.3.0",
		"nock": "^14.0.6",
		"pino-pretty": "^13.0.0",
		"prisma": "6.8.2",
		"rimraf": "^6.0.1",
		"sinon": "^20.0.0",
		"tsx": "^4.20.3",
		"typescript": "5.8.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	}
}
